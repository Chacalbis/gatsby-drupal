  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "run-s build:*",
    "build:pre-build": "run-p lint:*",
    "build:main": "gatsby build",
    "develop": "gatsby develop --host=0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md,scss}\"",
    "lint:prettier": "prettier --check \"**/*.{js,jsx,json,md,scss}\"",
    "lint": "run-p lint:*",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \" && exit 1"
