{
  "name": "ternum-generateur",
  "private": true,
  "description": "Static sites generator project using gatsby-source-drupal plugin",
  "version": "0.1.0",
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@brainhubeu/react-carousel": "^1.10.49",
    "@bumped-inc/gatsby-plugin-optional-chaining": "^1.0.0",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "gatsby": "^2.17.4",
    "gatsby-core-utils": "^1.0.28",
    "gatsby-image": "^2.2.29",
    "gatsby-plugin-lunr": "^1.5.2",
    "gatsby-plugin-manifest": "^2.2.23",
    "gatsby-plugin-offline": "^3.0.17",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-sass": "^2.1.20",
    "gatsby-plugin-sharp": "^2.2.31",
    "gatsby-source-drupal": "^3.3.10",
    "gatsby-source-filesystem": "^2.1.41",
    "gatsby-source-graphql": "^2.1.33",
    "gatsby-transformer-sharp": "^2.2.23",
    "icomoon-react": "^1.0.0",
    "moment-timezone": "^0.5.27",
    "node-sass": "^4.13.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-html-parser": "^2.0.2",
    "typeface-poppins": "0.0.72",
    "typeface-source-sans-pro": "0.0.75",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "10.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "run-s build:*",
    "build:pre-build": "run-p lint:*",
    "build:main": "gatsby build",
    "develop": "gatsby develop --host=0.0.0.0",
    "prettier": "prettier \"**/*.{json,md,scss,yaml,yml}\"",
    "start": "npm run develop",
    "serve": "gatsby serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write"
    ]
  }
}
